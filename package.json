{
  "name": "@travi/any",
  "description": "random data generator for when test data is insignificant",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "contributors": [
    {
      "name": "Trevor Richardson",
      "email": "npm@trevorrichardson.me",
      "url": "https://trevorrichardson.me"
    },
    {
      "name": "Zach Toben",
      "email": "zach.toben@gmail.com",
      "url": "https://ztoben.com/"
    },
    {
      "name": "Justin Waite",
      "email": "justindwaite@gmail.com",
      "url": "https://github.com/justinwaite"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/travi/any.git"
  },
  "bugs": {
    "url": "https://github.com/travi/any/issues"
  },
  "homepage": "https://github.com/travi/any#readme",
  "funding": {
    "url": "https://github.com/travi/any?sponsor=1"
  },
  "keywords": [
    "test",
    "random",
    "data",
    "chance"
  ],
  "exports": {
    "types": "./index.d.ts",
    "import": "./lib/any.mjs",
    "require": "./lib/any.js"
  },
  "main": "lib/any.js",
  "module": "lib/any.mjs",
  "types": "./index.d.ts",
  "scripts": {
    "clean": "rimraf coverage/ lib/",
    "build": "rollup -c",
    "lint:js": "eslint .",
    "lint:md": "remark . --frail",
    "lint:peer": "npm ls >/dev/null",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "mocha --recursive test/unit/",
    "test:types": "tsd",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "generate:md": "remark . --output",
    "coverage": "nyc run-s tests:unit",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test",
    "prepack": "npm run build",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/chance": "1.1.3",
    "chance": "1.1.11",
    "debug": "4.3.4",
    "lodash": "4.17.21",
    "mersenne-twister": "1.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-exponentiation-operator": "7.18.6",
    "@babel/register": "7.21.0",
    "@rollup/plugin-babel": "6.0.3",
    "@travi/babel-preset": "3.0.100",
    "@travi/eslint-config": "3.0.30",
    "@travi/eslint-config-mocha": "1.0.19",
    "c8": "7.14.0",
    "chai": "4.3.7",
    "commitlint-config-travi": "1.4.30",
    "coveralls": "3.1.1",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "formatio": "1.2.0",
    "globstar": "1.0.0",
    "husky": "8.0.3",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5",
    "proxyquire": "2.1.3",
    "referee": "1.2.0",
    "referee-sinon": "1.0.3",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "5.0.2",
    "remark-toc": "8.0.1",
    "rimraf": "5.0.1",
    "rollup": "3.23.0",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "15.1.0",
    "tsd": "0.28.1"
  }
}
