{
  "name": "@travi/any",
  "version": "0.0.0-semantically-released",
  "description": "random data generator for when test data is insignificant",
  "main": "lib/any.cjs.js",
  "module": "lib/any.es.js",
  "scripts": {
    "clean": "rimraf coverage/ lib/",
    "build": "rollup -c",
    "lint:js": "eslint .",
    "lint:md": "markdownlint **/*.md",
    "tests:unit": "mocha --recursive test/unit/",
    "test": "run-s clean lint:* tests:*",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "postpublish": "greenkeeper-postpublish",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/travi/any.git"
  },
  "keywords": [
    "test",
    "random",
    "data",
    "chance"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travi/any/issues"
  },
  "homepage": "https://github.com/travi/any#readme",
  "dependencies": {
    "chance": "1.0.4",
    "lodash": "4.17.2"
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "1.0.0-beta.2",
    "babel-cli": "6.18.0",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "coveralls": "2.11.15",
    "cz-conventional-changelog": "1.2.0",
    "formatio": "1.2.0",
    "greenkeeper-postpublish": "1.0.1",
    "husky": "0.12.0",
    "istanbul": "1.0.0-alpha.2",
    "markdownlint-cli": "0.2.0",
    "mocha": "3.2.0",
    "npm-run-all": "3.1.2",
    "proxyquire": "1.7.10",
    "referee": "1.2.0",
    "referee-sinon": "1.0.3",
    "rimraf": "2.5.4",
    "rollup": "0.37.0",
    "rollup-plugin-babel": "2.7.1",
    "semantic-release": "6.3.2",
    "sinon": "2.0.0-pre.4",
    "validate-commit-msg": "2.8.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
