{
  "name": "@travi/any",
  "version": "0.0.0-semantically-released",
  "description": "random data generator for when test data is insignificant",
  "main": "lib/any.cjs.js",
  "module": "lib/any.es.js",
  "scripts": {
    "clean": "rimraf coverage/ lib/",
    "build": "rollup -c",
    "docs:toc": "markdown-toc --no-firsth1 --maxdepth 1 --bullets \"*\" docs/API.md",
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint *.md '**/*.md'",
    "tests:unit": "mocha --recursive test/unit/",
    "test": "run-s clean lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test",
    "prepack": "npm run build",
    "precommit": "npm test",
    "commitmsg": "commitlint -e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/travi/any.git"
  },
  "keywords": [
    "test",
    "random",
    "data",
    "chance"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "contributors": [
    {
      "name": "Trevor Richardson",
      "email": "tr@trevorrichardson.me",
      "url": "https://twitter.com/intelxdesign"
    },
    {
      "name": "Zach Toben",
      "email": "zach.toben@gmail.com",
      "url": "https://ztoben.com/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travi/any/issues"
  },
  "homepage": "https://github.com/travi/any#readme",
  "dependencies": {
    "chance": "1.0.17",
    "debug": "4.1.1",
    "lodash": "4.17.11",
    "mersenne-twister": "1.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-exponentiation-operator": "7.2.0",
    "@babel/register": "7.0.0",
    "@travi/babel-preset": "2.0.0",
    "@travi/eslint-config-travi": "1.7.0",
    "chai": "4.2.0",
    "commitlint-config-travi": "1.2.32",
    "coveralls": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "formatio": "1.2.0",
    "globstar": "1.0.0",
    "husky": "1.3.1",
    "markdown-toc": "1.2.0",
    "markdownlint-cli": "0.13.0",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.5",
    "nyc": "13.1.0",
    "proxyquire": "2.1.0",
    "referee": "1.2.0",
    "referee-sinon": "1.0.3",
    "rimraf": "2.6.3",
    "rollup": "0.68.2",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-babel": "4.3.0",
    "sinon": "7.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
