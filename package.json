{
  "name": "@travi/any",
  "version": "0.0.0-semantically-released",
  "description": "random data generator for when test data is insignificant",
  "main": "lib/any.cjs.js",
  "module": "lib/any.es.js",
  "scripts": {
    "clean": "rimraf coverage/ lib/",
    "build": "rollup -c",
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:unit": "mocha --recursive test/unit/",
    "test": "run-s clean lint:* coverage",
    "coverage": "nyc run-s tests:unit",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "postpublish": "greenkeeper-postpublish",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/travi/any.git"
  },
  "keywords": [
    "test",
    "random",
    "data",
    "chance"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travi/any/issues"
  },
  "homepage": "https://github.com/travi/any#readme",
  "dependencies": {
    "chance": "1.0.10",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "1.2.13",
    "babel-cli": "6.24.1",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-register": "6.24.1",
    "chai": "4.0.2",
    "coveralls": "2.13.1",
    "cz-conventional-changelog": "2.0.0",
    "formatio": "1.2.0",
    "globstar": "1.0.0",
    "greenkeeper-postpublish": "1.0.1",
    "husky": "0.13.4",
    "markdownlint-cli": "0.3.0",
    "mocha": "3.4.2",
    "npm-run-all": "4.0.2",
    "nyc": "11.0.2",
    "proxyquire": "1.8.0",
    "referee": "1.2.0",
    "referee-sinon": "1.0.3",
    "rimraf": "2.6.1",
    "rollup": "0.42.0",
    "rollup-plugin-babel": "2.7.1",
    "semantic-release": "6.3.6",
    "sinon": "2.3.5",
    "validate-commit-msg": "2.12.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
